RewriteEngine on
RewriteBase /
RewriteRule ^(register|login|logout)?$ index.php?a=$1 [L]
RewriteRule ^login/auth?$ index.php?a=login&type=auth [L]
RewriteRule ^password/reset?$ index.php?a=password&b=reset [L]
RewriteRule ^password/change/(.*)?$ index.php?a=password&b=change&hash=$1 [L]
RewriteRule ^email-verify/(.*)?$ index.php?a=email_verify&hash=$1 [L]
RewriteRule ^account/(summary|ref|activity|settings|supports)?$ index.php?a=account&b=$1 [L]
RewriteRule ^account/money/(offerwalls|reward|chart|deposit|withdrawal|membership)?$ index.php?a=account&b=money&c=$1 [L]
RewriteRule ^account/transaction/(.*)?$ index.php?a=account&b=transaction&id=$1 [L]
RewriteRule ^account/settings/(profile|change_password|wallet_passphrase|2fa|verification|logs)?$ index.php?a=account&b=settings&c=$1 [L]
RewriteRule ^account/activity/(.*)?$ index.php?a=account&b=activity&page=$1 [L]
RewriteRule ^deposit/(.*)/success?$ index.php?a=deposit&b=success&id=$1 [L]
RewriteRule ^deposit/(.*)/fail?$ index.php?a=deposit&b=fail&id=$1 [L]
RewriteRule ^account/logs/(.*)?$ index.php?a=account&b=logs&page=$1 [L]
RewriteRule ^page/(.*)?$ index.php?a=page&prefix=$1 [L]
RewriteRule ^account/support/(.*)?$ index.php?a=account&b=supports&c=support&id=$1 [L]
RewriteRule ^account/supports/(.*)?$ index.php?a=account&b=supports&page=$1 [L]
RewriteRule ^account/open/support/(.*)?$ index.php?a=account&b=supports&c=open&txid=$1 [L]
RewriteRule ^account/escalate/support/(.*)?$ index.php?a=account&b=supports&c=escalate&id=$1 [L]
RewriteRule ^account/close/support/(.*)?$ index.php?a=account&b=supports&c=close&id=$1 [L]